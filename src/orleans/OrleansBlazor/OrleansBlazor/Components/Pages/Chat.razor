@page "/chat"
@rendermode InteractiveServer
@inject IClusterClient ClusterClient

<h1>Chat</h1>

<input @bind="@Username" />
<input @bind="@Message" />
<button @onclick="SendMessage">Send Message</button>

<ul>
    @foreach (var message in Messages)
    {
        <li>@message.ToString()</li>
    }
</ul>